<factory>
	<workcenter id="ws1">
		<workcenter id="wc1.1">
			<operation ref="shaftprocessing"></operation>
		</workcenter>
		<workcenter id="wc1.2">
			<operation ref="shaftprocessing"></operation>
		</workcenter>
		<workcenter id="swh-1">
			Income warehouse
			<operation ref="orderblankshaft"/>
			<operation ref="bookblankshaft"/>
		</workcenter>
		<workcenter id="pwh-1">
			Outcome warehouse
		</workcenter>
	</workcenter>
	<workcenter id="ws2">
		<operation ref="transportinws2"/>
		<workcenter id="wc2.1">
			<operation ref="shaftwheelassemble"></operation>
		</workcenter>
		<workcenter id="wc2.2">
			<operation ref="qualitycheck"></operation>
		</workcenter>
		<workcenter id="wc2.3">
			<operation ref="shaftwheelassemble"></operation>
		</workcenter>
		<workcenter id="pwh-2">
		</workcenter>
		<workcenter id="swh-2">
		</workcenter>
	</workcenter>
	<workcenter id="cwh">
		Central Warehouse
		<operation ref="deliverytocwh"/>
		<operation ref="suppliercontraction"/>
		<operation ref="supplierselection"/>
	</workcenter>
	<workcenter id="pwh">
		Finishing products warehouse
	</workcenter>
	<route id="1" from="cwh" to="swh-1" reverse="1"></route>
	<route id="2" from="cwh" to="swh-2" reverse="1"></route>
	<route id="3" from="wc2.1" to="wc2.2" reverse="1"></route>
	<route id="4" from="wc2.2" to="pwh-2" reverse="0"></route>
	<route id="5" from="pwh-2" to="pwh" reverse="0"></route>
	<route id="6" from="pwh-1" to="swh-2" reverse="0"></route>
	<route id="7" from="swh-1" to="wc1.1" reverse="0"></route>
	<route id="8" from="swh-1" to="wc1.2" reverse="0"></route>
	<route id="9" from="swh-2" to="wc2.1" reverse="0"></route>
	<route id="10" from="wc1.1" to="pwh-1" reverse="1"></route>
	<route id="11" from="wc1.2" to="pwh-1" reverse="0"></route>
	<route id="12" from="pwh-2" to="pwh" reverse="0"></route>
	<route id="13" from="pwh-2" to="customer*" reverse="0"></route>
	<route id="14" from="pwh" to="customer*" reverse="0"></route>
</factory>